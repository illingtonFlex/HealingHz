<?xml version='1.0' encoding='utf-8'?>
<widget id="us.healinghz.mobile" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>HealingHz</name>
    <description>
        A mobile app for improving pitch perception.
    </description>
    <author email="contact@healinghz.us" href="http://healinghz.us">
        @JasonCNorman
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <platform name="android">
        <preference name="Orientation" value="landscape" />
        <preference name="Fullscreen" value="true" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <preference name="Orientation" value="landscape" />
        <preference name="Fullscreen" value="true" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
